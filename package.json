{
  "name": "chaos",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:updateSnap": "turbo run test:updateSnap",
    "publish-packages": "turbo run build test && changeset version && changeset publish",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@cloud-dragon/eslint-config": "workspace:*",
    "@cloud-dragon/prettier-config": "workspace:*",
    "@cloud-dragon/tsconfig": "workspace:*",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@swc/core": "^1.3.36",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "acorn-jsx": "^5.3.2",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "lodash": "^4.17.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-native-web": "^0.19.4",
    "remark-gfm": "^3.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-swc": "^0.2.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "storybook": "^7.0.0",
    "ts-jest": "^29.0.5",
    "turbo": "latest",
    "typescript": "^5.0.4",
    "vite": "^4.1.0"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.17.1"
}
